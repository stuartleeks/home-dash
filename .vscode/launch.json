{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "dash-api-go",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/dash-api-go"
		},
		{
			"name": "Python Debugger: api",
			"type": "debugpy",
			"request": "launch",
			"module": "uvicorn",
			"cwd": "${workspaceFolder}/dash-api",
			"args": ["main:app", "--reload"]
		},
		{
			"name": "Python Debugger: leaf-status",
			"type": "debugpy",
			"request": "launch",
			"program": "leaf-status/get-leaf-summary.py",
			"console": "integratedTerminal"
		}
	]
}